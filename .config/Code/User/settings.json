{
	"editor.lineNumbers": "relative",
	"window.zoomLevel": 2,
	"vim.useSystemClipboard": true,
	"vim.hlsearch": true,
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<space>"
			],
			"after": [
				"g", "t"
			]
		},
		{
			"before": [
				"+"
			],
			"commands": [
				":",
				"<C-f>",
				"i",
				"S",
				"/"
			]
		},
		{
			"before": [
				"k"
			],
			"after": [
				"g",
				"k"
			]
		},
		{
			"before": [
				"$"
			],
			"after": [
				"g",
				"$"
			]
		},
		{
			"before": [
				"^"
			],
			"after": [
				"g",
				"^"
			]
		},
		{
			"before": [
				"J"
			],
			"after": [
				"g",
				"J"
			]
		},
		{
			"before": [
				"m",
				"w"
			],
			"after": [
				"<leader>",
				"w"
			]
		},
		{"before": ["mb"], "after": ["<leader>", "b"]},
		{"before": ["e"], "after": ["<leader>", "e"]},
		{
            "before": ["]"],
            "commands": [
                "workbench.action.editor.nextChange",
            ],
            "silent": true
        },
		{"before": ["t", "a"], "after": ["i", "x", "<Esc>", "d", "l", "'", "A", "'", "."]},
		{"before": ["T", "a"], "after": ["m", "A"]},
		{"before": ["t", "b"], "after": ["i", "x", "<Esc>", "d", "l", "'", "B", "'", "."]},
		{"before": ["T", "b"], "after": ["m", "B"]},
		{"before": ["t", "c"], "after": ["i", "x", "<Esc>", "d", "l", "'", "C", "'", "."]},
		{"before": ["T", "c"], "after": ["m", "C"]},
		{"before": ["t", "d"], "after": ["i", "x", "<Esc>", "d", "l", "'", "D", "'", "."]},
		{"before": ["T", "d"], "after": ["m", "D"]},
		{"before": ["t", "e"], "after": ["i", "x", "<Esc>", "d", "l", "'", "E", "'", "."]},
		{"before": ["T", "e"], "after": ["m", "E"]},
		{"before": ["t", "f"], "after": ["i", "x", "<Esc>", "d", "l", "'", "F", "'", "."]},
		{"before": ["T", "f"], "after": ["m", "F"]},
		{"before": ["t", "g"], "after": ["i", "x", "<Esc>", "d", "l", "'", "G", "'", "."]},
		{"before": ["T", "g"], "after": ["m", "G"]},
		{"before": ["t", "h"], "after": ["i", "x", "<Esc>", "d", "l", "'", "H", "'", "."]},
		{"before": ["T", "h"], "after": ["m", "H"]},
		{"before": ["t", "i"], "after": ["i", "x", "<Esc>", "d", "l", "'", "I", "'", "."]},
		{"before": ["T", "i"], "after": ["m", "I"]},
		{"before": ["t", "j"], "after": ["i", "x", "<Esc>", "d", "l", "'", "J", "'", "."]},
		{"before": ["T", "j"], "after": ["m", "J"]},
		{"before": ["t", "k"], "after": ["i", "x", "<Esc>", "d", "l", "'", "K", "'", "."]},
		{"before": ["T", "k"], "after": ["m", "K"]},
		{"before": ["t", "l"], "after": ["i", "x", "<Esc>", "d", "l", "'", "L", "'", "."]},
		{"before": ["T", "l"], "after": ["m", "L"]},
		{"before": ["t", "m"], "after": ["i", "x", "<Esc>", "d", "l", "'", "M", "'", "."]},
		{"before": ["T", "m"], "after": ["m", "M"]},
		{"before": ["t", "n"], "after": ["i", "x", "<Esc>", "d", "l", "'", "N", "'", "."]},
		{"before": ["T", "n"], "after": ["m", "N"]},
		{"before": ["t", "o"], "after": ["i", "x", "<Esc>", "d", "l", "'", "O", "'", "."]},
		{"before": ["T", "o"], "after": ["m", "O"]},
		{"before": ["t", "p"], "after": ["i", "x", "<Esc>", "d", "l", "'", "P", "'", "."]},
		{"before": ["T", "p"], "after": ["m", "P"]},
		{"before": ["t", "q"], "after": ["i", "x", "<Esc>", "d", "l", "'", "Q", "'", "."]},
		{"before": ["T", "q"], "after": ["m", "Q"]},
		{"before": ["t", "r"], "after": ["i", "x", "<Esc>", "d", "l", "'", "R", "'", "."]},
		{"before": ["T", "r"], "after": ["m", "R"]},
		{"before": ["t", "s"], "after": ["i", "x", "<Esc>", "d", "l", "'", "S", "'", "."]},
		{"before": ["T", "s"], "after": ["m", "S"]},
		{"before": ["t", "t"], "after": ["i", "x", "<Esc>", "d", "l", "'", "T", "'", "."]},
		{"before": ["T", "t"], "after": ["m", "T"]},
		{"before": ["t", "u"], "after": ["i", "x", "<Esc>", "d", "l", "'", "U", "'", "."]},
		{"before": ["T", "u"], "after": ["m", "U"]},
		{"before": ["t", "v"], "after": ["i", "x", "<Esc>", "d", "l", "'", "V", "'", "."]},
		{"before": ["T", "v"], "after": ["m", "V"]},
		{"before": ["t", "w"], "after": ["i", "x", "<Esc>", "d", "l", "'", "W", "'", "."]},
		{"before": ["T", "w"], "after": ["m", "W"]},
		{"before": ["t", "x"], "after": ["i", "x", "<Esc>", "d", "l", "'", "X", "'", "."]},
		{"before": ["T", "x"], "after": ["m", "X"]},
		{"before": ["t", "y"], "after": ["i", "x", "<Esc>", "d", "l", "'", "Y", "'", "."]},
		{"before": ["T", "y"], "after": ["m", "Y"]},
		{"before": ["t", "z"], "after": ["i", "x", "<Esc>", "d", "l", "'", "Z", "'", "."]},
		{"before": ["T", "z"], "after": ["m", "Z"]},

	],
	"vim.operatorPendingModeKeyBindingsNonRecursive": [
		{"before": ["m", "i","w"], "after": ["i", "\\", "w"]},
	],
	"vim.normalModeKeyBindings": [
		{"before": ["mw"], "after": ["\\", "w"]},
		{"before": ["mb"], "after": ["<leader>", "b"]},
		{"before": ["me"], "after": ["\\", "e"]},
		{"before": ["s", "a"], "after": ["y", "s"]},
		{"before": ["d", "a"], "after": ["d", "s"]},
		{"before": ["s", "r"], "after": ["c", "s"]},
		{"before": ["]"], "commands": [ "workbench.action.compareEditor.nextChange" ]}
	],
	"vim.visualModeKeyBindings": [
		{"before": ["s", "a"], "after": ["y", "s"]},
		{"before": ["d", "a"], "after": ["d", "s"]},
		{"before": ["s", "r"], "after": ["c", "s"]},
		{"before": ["s", "a"], "after": ["S"]},
	],

	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": [
				"V"
			],
			"after": [
				"^",
				"o",
				"$"
			]
		},
		{
			"before": [
				"p"
			],
			"after": [
				"p",
				"g",
				"v",
				"y"
			]
		},
		{
			"before": [
				"J"
			],
			"after": [
				"g",
				"J"
			]
		},
		{"before": ["m", "b"], "after": ["\\", "b"]},
		{"before": ["m", "e"], "after": ["\\", "e"]},
		{"before": ["m", "w"], "after": ["\\", "w"]},
		{"before": ["s", "a"], "after": ["y", "a"]},
	],
	"vim.insertModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<C-u>"
			],
			"after": [
				"<Esc>",
				"<C-u>",
				"i"
			]
		},
		{
			"before": [
				"<C-d>"
			],
			"after": [
				"<Esc>",
				"<C-d>",
				"i"
			]
		},
		{
			"before": [
				"<C-v>"
			],
			"after": [
				"<Esc>",
				"v",
				"<C-v>"
			]
		}
	],
	"vim.camelCaseMotion.enable": true,
	"vim.surround": true,
	"[python]": {
		"editor.formatOnType": true
	}
}