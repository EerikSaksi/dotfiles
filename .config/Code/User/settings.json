{
	"editor.lineNumbers": "relative",
	"vim.useSystemClipboard": true,
	"vim.hlsearch": true,
	"vim.sneak": true,
	//"vim.sneakReplacesF": true,
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				"+"
			],
			"commands": [
				":",				"<C-f>",				"i",				"S",				"/"
			]
		},
		{
			"before": [
				"k"
			],
			"after": [
				"g",
				"k"
			]
		},
		{
			"before": [
				"$"
			],
			"after": [
				"g",
				"$"
			]
		},
		{
			"before": [
				"^"
			],
			"after": [
				"g",
				"^"
			]
		},
		{
			"before": [
				"J"
			],
			"after": [
				"g",
				"J"
			]
		},
		{
			"before": [
				"m",
				"w"
			],
			"after": [
				"<leader>",
				"w"
			]
		},
		{"before": ["mb"], "after": ["<leader>", "b"]},
		{"before": ["e"], "after": ["<leader>", "e"]},
		{
            "before": ["]"],
            "commands": [
                "workbench.action.editor.nextChange",
            ],
            "silent": true
        },
		{
            "before": ["<space>"],
            "commands": [
				"editor.action.formatDocument"
            ],
            "silent": true
        },
		{
            "before": ["<C-g>", "d"],
            "commands": [
				"git.openChange"
            ],
            "silent": true
        },

		{
            "before": ["<C-g>", "l"],
            "commands": [
				"workbench.view.scm"
			],
        },

		{
            "before": ["<C-g>", "L"],
            "commands": [
				"gitlens.compareWorkingWith"
			],
        },
		{
			"before": ["<C-w>", "l"],
			"commands": [
				"workbench.action.compareEditor.focusPrimarySide"
			]
		},
		{
			"before": ["<C-w>", "h"],
			"commands": [
				"workbench.action.compareEditor.focusSecondarySide"
			]
		},
		{
			"before": ["<C-g>", "h"],
			"commands": [
				"workbench.action.files.revert"
			]
		},
		{"before": ["f"], "after": [ "s" ]},
		{"before": ["F"], "after": [ "S" ]},
		{"before": ["g", "z"], "commands": [ "workbench.action.findInFiles" ]},
		{"before": ["t", "a"], "after": ["m", "a", "'", "A", "'", "a"]},
		{"before": ["T", "a"], "after": ["m", "A"]},
		{"before": ["t", "b"], "after": ["m", "a", "'", "B", "'", "a"]},
		{"before": ["T", "b"], "after": ["m", "B"]},
		{"before": ["t", "c"], "after": ["m", "a", "'", "C", "'", "a"]},
		{"before": ["T", "c"], "after": ["m", "C"]},
		{"before": ["t", "d"], "after": ["m", "a", "'", "D", "'", "a"]},
		{"before": ["T", "d"], "after": ["m", "D"]},
		{"before": ["t", "e"], "after": ["m", "a", "'", "E", "'", "a"]},
		{"before": ["T", "e"], "after": ["m", "E"]},
		{"before": ["t", "f"], "after": ["m", "a", "'", "F", "'", "a"]},
		{"before": ["T", "f"], "after": ["m", "F"]},
		{"before": ["t", "g"], "after": ["m", "a", "'", "G", "'", "a"]},
		{"before": ["T", "g"], "after": ["m", "G"]},
		{"before": ["t", "h"], "after": ["m", "a", "'", "H", "'", "a"]},
		{"before": ["T", "h"], "after": ["m", "H"]},
		{"before": ["t", "i"], "after": ["m", "a", "'", "I", "'", "a"]},
		{"before": ["T", "i"], "after": ["m", "I"]},
		{"before": ["t", "j"], "after": ["m", "a", "'", "J", "'", "a"]},
		{"before": ["T", "j"], "after": ["m", "J"]},
		{"before": ["t", "k"], "after": ["m", "a", "'", "K", "'", "a"]},
		{"before": ["T", "k"], "after": ["m", "K"]},
		{"before": ["t", "l"], "after": ["m", "a", "'", "L", "'", "a"]},
		{"before": ["T", "l"], "after": ["m", "L"]},
		{"before": ["t", "m"], "after": ["m", "a", "'", "M", "'", "a"]},
		{"before": ["T", "m"], "after": ["m", "M"]},
		{"before": ["t", "n"], "after": ["m", "a", "'", "N", "'", "a"]},
		{"before": ["T", "n"], "after": ["m", "N"]},
		{"before": ["t", "o"], "after": ["m", "a", "'", "O", "'", "a"]},
		{"before": ["T", "o"], "after": ["m", "O"]},
		{"before": ["t", "p"], "after": ["m", "a", "'", "P", "'", "a"]},
		{"before": ["T", "p"], "after": ["m", "P"]},
		{"before": ["t", "q"], "after": ["m", "a", "'", "Q", "'", "a"]},
		{"before": ["T", "q"], "after": ["m", "Q"]},
		{"before": ["t", "r"], "after": ["m", "a", "'", "R", "'", "a"]},
		{"before": ["T", "r"], "after": ["m", "R"]},
		{"before": ["t", "s"], "after": ["m", "a", "'", "S", "'", "a"]},
		{"before": ["T", "s"], "after": ["m", "S"]},
		{"before": ["t", "t"], "after": ["m", "a", "'", "T", "'", "a"]},
		{"before": ["T", "t"], "after": ["m", "T"]},
		{"before": ["t", "u"], "after": ["m", "a", "'", "U", "'", "a"]},
		{"before": ["T", "u"], "after": ["m", "U"]},
		{"before": ["t", "v"], "after": ["m", "a", "'", "V", "'", "a"]},
		{"before": ["T", "v"], "after": ["m", "V"]},
		{"before": ["t", "w"], "after": ["m", "a", "'", "W", "'", "a"]},
		{"before": ["T", "w"], "after": ["m", "W"]},
		{"before": ["t", "x"], "after": ["m", "a", "'", "X", "'", "a"]},
		{"before": ["T", "x"], "after": ["m", "X"]},
		{"before": ["t", "y"], "after": ["m", "a", "'", "Y", "'", "a"]},
		{"before": ["T", "y"], "after": ["m", "Y"]},
		{"before": ["t", "z"], "after": ["m", "a", "'", "Z", "'", "a"]},
		{"before": ["T", "z"], "after": ["m", "Z"]},
	],
	"vim.operatorPendingModeKeyBindingsNonRecursive": [
		{"before": ["i","w"], "after": ["i", "\\", "w"]},
	],
	"vim.normalModeKeyBindings": [
		{"before": ["w"], "after": ["\\", "w"]},
		{"before": ["b"], "after": ["\\", "b"]},
		{"before": ["e"], "after": ["\\", "e"]},
		{"before": ["s", "a"], "after": ["y", "s"]},
		{"before": ["s", "d"], "after": ["d", "s"]},
		{"before": ["s", "r"], "after": ["c", "s"]},
		{"before": ["]"], "commands": [ "workbench.action.compareEditor.nextChange" ]},
		{"before": ["R"], "commands": [ "editor.action.rename" ]},
	],
	"vim.visualModeKeyBindings": [
		{"before": ["s", "a"], "after": ["S"]},
		{"before": ["d", "a"], "after": ["d", "s"]},
		{"before": ["s", "r"], "after": ["c", "s"]},
	],
	"vim.handleKeys": {
		"<C-d>": true,
		"<C-s>": false,
		"<C-z>": false,
		"<C-w>": true
	},

	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": [
				"V"
			],
			"after": [
				"^",
				"o",
				"$"
			]
		},
		{
			"before": [
				"p"
			],
			"after": [
				"p",
				"g",
				"v",
				"y"
			]
		},
		{
			"before": [
				"J"
			],
			"after": [
				"g",
				"J"
			]
		},
		{"before": ["m", "b"], "after": ["\\", "b"]},
		{"before": ["m", "e"], "after": ["\\", "e"]},
		{"before": ["m", "w"], "after": ["\\", "w"]},
		{"before": ["s", "a"], "after": ["y", "a"]},
		{"before": ["<space>"], "commands": ["editor.action.formatSelection"]},
	],
	"vim.insertModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<C-u>"
			],
			"after": [
				"<Esc>",
				"<C-u>",
				"i"
			]
		},
		{
			"before": [
				"<C-d>"
			],
			"after": [
				"<Esc>",
				"<C-d>",
				"i"
			]
		},
		{
			"before": [
				"<C-v>"
			],
			"after": [
				"<Esc>",
				"v",
				"<C-v>"
			]
		}
	],
	"vim.camelCaseMotion.enable": true,
	"vim.surround": true,
	"[python]": {
		"editor.formatOnType": true
	},
	"workbench.editor.showTabs": false,
	"workbench.colorTheme": "SynthWave '84",
	"editor.inlineSuggest.enabled": true,
	"gitlens.currentLine.enabled": false,
	"gitlens.hovers.currentLine.over": "line",
	"gitlens.codeLens.enabled": false,
	"window.zoomLevel": -1,
}